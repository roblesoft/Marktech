<!DOCTYPE html>
<html>
  <head>
    <title>Marktech</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <div class="container">
    	<header>
    		<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
			  <%= link_to "Marktech","/inicio", method: :get, :class => "navbar-brand"%>
			  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
			    <span class="navbar-toggler-icon"></span>
			  </button>
			  <div class="collapse navbar-collapse" id="navbarNavDropdown">
			    <ul class="navbar-nav">
			      <li class="nav-item">
					<%= link_to "Fuentes de poder","/welcome/fuentes", method: :get, :class => "nav-link"%>
			      </li>
			      <li class="nav-item">
			        <%= link_to "protos","/welcome/protos", method: :get, :class => "nav-link"%>
			      </li class="nav-item">
			        <%= link_to "Arduinos","/welcome/arduinos", method: :get, :class => "nav-link"%> 		
			      </li>
			      </li class="nav-item">
			        <%= link_to "Publicado","/products", method: :get, :class => "nav-link"%>
			      </li>
			    </ul>
			  </div>
			  <div class="btn-group">
			  	<%= link_to "Publicar", "/products/new", method: :get, :class => "btn btn-primary"%>
			  	<a class="btn btn-primary" href="#" role="button">iniciar secion</a>
			  </div>
			</nav>
    	</header>
    	<%= yield %>
    </div>
  </body>
</html>
